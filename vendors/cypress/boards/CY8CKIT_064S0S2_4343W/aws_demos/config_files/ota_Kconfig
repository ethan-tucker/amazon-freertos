if BOARD_CY8CKIT_064S0S2_4343W
    config STACK_SIZE_CY8CKIT_064S0S2_4343W
        int "Stack size"
        default 6144
        depends on !STACK_SIZE_SET_DEMO

    config LOG2_FILE_BLOCK_SIZE_CY8CKIT_064S0S2_4343W
        int "Log 2 file block size"
        default 10
        depends on !LOG2_FILE_BLOCK_SIZE_SET_DEMO
        
    config SELF_TEST_RESPONSE_WAIT_MS_CY8CKIT_064S0S2_4343W
        int "Self test response wait (milliseconds)"
        default 16000
        depends on !SELF_TEST_RESPONSE_WAIT_MS_SET_DEMO

    config FILE_REQUEST_WAIT_MS_CY8CKIT_064S0S2_4343W
        int "File request wait (milliseconds)"
        default 2500
        depends on !FILE_REQUEST_WAIT_MS_SET_DEMO

    config OTA_AGENT_PRIORITY_CY8CKIT_064S0S2_4343W
        int "OTA agent priority"
        default 2
        help
            0 is the lowest priority.
        depends on !OTA_AGENT_PRIORITY_SET_DEMO

    config MAX_THINGNAME_LEN_CY8CKIT_064S0S2_4343W
        int "Max thingname length"
        default 64
        depends on !MAX_THINGNAME_LEN_SET_DEMO

    config MAX_NUM_BLOCKS_REQUEST_CY8CKIT_064S0S2_4343W
        int "Max num of blocks requested"
        default 128
        depends on !MAX_NUM_BLOCKS_REQUEST_SET_DEMO

    config MAX_NUM_REQUEST_MOMENTUM_CY8CKIT_064S0S2_4343W
        int "Max num request momentum"
        default 32
        depends on !MAX_NUM_REQUEST_MOMENTUM_SET_DEMO

    config MAX_NUM_OTA_DATA_BUFFERS_CY8CKIT_064S0S2_4343W
        int "Max num OTA data buffers"
        default 2
        depends on !MAX_NUM_OTA_DATA_BUFFERS_SET_DEMO

    config ALLOW_DOWNGRADE_CY8CKIT_064S0S2_4343W
        bool "Allow downgrades"
        default 0
        depends on !ALLOW_DOWNGRADE_SET_DEMO

    if !ENABLED_CONTROL_PROTOCOLS_SET_DEMO
    choice ENABLED_CONTROL_PROTOCOLS_CY8CKIT_064S0S2_4343W
        bool "Enabled control protocols"

        config ENABLED_CONTROL_PROTOCOLS_MQTT_CY8CKIT_064S0S2_4343W
            bool "OTA data over MQTT"

        config ENABLED_CONTROL_PROTOCOLS_HTTP_CY8CKIT_064S0S2_4343W
            bool "OTA data over HTTP"

        config ENABLED_CONTROL_PROTOCOLS_HTTP_AND_MQTT_CY8CKIT_064S0S2_4343W
            bool "OTA data over HTTP and MQTT"
    endchoice
    endif

    if !ENABLED_DATA_PROTOCOLS_SET_DEMO
    choice ENABLED_DATA_PROTOCOLS_CY8CKIT_064S0S2_4343W
        bool "Enabled data protocols"

        config ENABLED_DATA_PROTOCOLS_HTTP_AND_MQTT_CY8CKIT_064S0S2_4343W
            bool "OTA data over HTTP and MQTT"

        config ENABLED_DATA_PROTOCOLS_HTTP_CY8CKIT_064S0S2_4343W
            bool "OTA data over HTTP"

        config ENABLED_DATA_PROTOCOLS_MQTT_CY8CKIT_064S0S2_4343W
            bool "OTA data over MQTT"
    endchoice
    endif

    if !PRIMARY_DATA_PROTOCOL_SET_DEMO
    choice PRIMARY_DATA_PROTOCOL_CY8CKIT_064S0S2_4343W
        bool "Select a primary data protocol"

        config PRIMARY_DATA_PROTOCOLS_HTTP_CY8CKIT_064S0S2_4343W
            bool "HTTP as primary data protocol"
            depends on (ENABLED_DATA_PROTOCOLS_HTTP_AND_MQTT_CY8CKIT_064S0S2_4343W || ENABLED_DATA_PROTOCOLS_HTTP_CY8CKIT_064S0S2_4343W)

        config PRIMARY_DATA_PROTOCOLS_MQTT_CY8CKIT_064S0S2_4343W
            bool "MQTT as primary data protocol"
            depends on (ENABLED_DATA_PROTOCOLS_HTTP_AND_MQTT_CY8CKIT_064S0S2_4343W || ENABLED_DATA_PROTOCOLS_MQTT_CY8CKIT_064S0S2_4343W)
    endchoice
    endif

    # for each variable that is set add a select statement here
    # formatted <variable name - board suffix>_SET
    config UPDATE_ALL_DEFAULT_DUMMY_VARIABLES
        bool
        default y
        select STACK_SIZE_SET_BOARD
        select LOG2_FILE_BLOCK_SIZE_SET_BOARD
        select SELF_TEST_RESPONSE_WAIT_MS_SET_BOARD
        select FILE_REQUEST_WAIT_MS_SET_BOARD
        select OTA_AGENT_PRIORITY_SET_BOARD
        select MAX_THINGNAME_LEN_SET_BOARD
        select MAX_NUM_BLOCKS_REQUEST_SET_BOARD
        select MAX_NUM_REQUEST_MOMENTUM_SET_BOARD
        select MAX_NUM_OTA_DATA_BUFFERS_SET_BOARD
        select ALLOW_DOWNGRADE_SET_BOARD
        select ENABLED_CONTROL_PROTOCOLS_SET_BOARD
        select ENABLED_DATA_PROTOCOLS_SET_BOARD
        select PRIMARY_DATA_PROTOCOL_SET_BOARD

endif